    ' Helper: create a panel representing a room card (rounded corners + labels)
    Private Function CreateRoomCard(title As String, count As String, time As String) As Panel
        Dim card As New Panel()
        card.Size = New System.Drawing.Size(488, 120)
        card.Margin = New Padding(6)
        card.BackColor = System.Drawing.Color.FromArgb(CType(CType(204, Byte), Integer), CType(CType(247, Byte), Integer), CType(CType(253, Byte), Integer))
        card.Padding = New Padding(12)

        ' Title
        Dim lblTitle As New Label()
        lblTitle.AutoSize = False
        lblTitle.Size = New System.Drawing.Size(440, 28)
        lblTitle.Text = title
        lblTitle.ForeColor = System.Drawing.Color.FromArgb(CType(CType(30, Byte), Integer), CType(CType(30, Byte), Integer), CType(CType(30, Byte), Integer))
        lblTitle.Location = New System.Drawing.Point(8, 8)
        lblTitle.Font = New System.Drawing.Font("Yu Gothic UI", 11.0!)

        ' Count (left bottom)
        Dim lblCount As New Label()
        lblCount.AutoSize = False
        lblCount.Size = New System.Drawing.Size(200, 24)
        lblCount.Text = count
        lblCount.Location = New System.Drawing.Point(8, 72)
        lblCount.ForeColor = System.Drawing.Color.FromArgb(CType(CType(50, Byte), Integer), CType(CType(50, Byte), Integer), CType(CType(50, Byte), Integer))
        lblCount.Font = New System.Drawing.Font("Yu Gothic UI", 10.0!)

        ' Time (right bottom)
        Dim lblTime As New Label()
        lblTime.AutoSize = False
        lblTime.Size = New System.Drawing.Size(200, 24)
        lblTime.Text = time
        lblTime.TextAlign = System.Drawing.ContentAlignment.MiddleRight
        lblTime.Location = New System.Drawing.Point(288, 72)
        lblTime.ForeColor = System.Drawing.Color.FromArgb(CType(CType(50, Byte), Integer), CType(CType(50, Byte), Integer), CType(CType(50, Byte), Integer))
        lblTime.Font = New System.Drawing.Font("Yu Gothic UI", 10.0!)

        card.Controls.Add(lblTitle)
        card.Controls.Add(lblCount)
        card.Controls.Add(lblTime)

        ' rounded corners via GraphicsPath -> Region
        Dim r As Integer = 12
        Dim w As Integer = card.Width
        Dim h As Integer = card.Height
        Using gp As New System.Drawing.Drawing2D.GraphicsPath()
            gp.AddArc(0, 0, r * 2, r * 2, 180, 90)
            gp.AddArc(w - r * 2, 0, r * 2, r * 2, 270, 90)
            gp.AddArc(w - r * 2, h - r * 2, r * 2, r * 2, 0, 90)
            gp.AddArc(0, h - r * 2, r * 2, r * 2, 90, 90)
            gp.CloseFigure()
            card.Region = New Region(gp)
        End Using

        Return card
    End Function

End Class